@model IEnumerable<Rkna_Project.Models.Governorate_Table>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<h2>All Governorate</h2>

<p>
    @Html.ActionLink("New Governorate", "Create", null, new { @class = "btn btn-primary btn-rounded btn-large" })
</p>
<table class="table ">
    <tr>
        <th>
            @Html.Label("Governorate Name")
        </th>
        <th>
            @Html.Label("Governorate Description")  
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Gov_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gov_Desc)
            </td>
            <td>
               
                @Html.ActionLink("Details", "Details", new { id = item.Gov_ID }, new { @class = "btn btn-primary btn-rounded btn-large" })
              </td>
        </tr>
    }

</table>